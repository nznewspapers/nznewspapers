{% from "macros.njk" import newspaperLine %}

<div class="multicolumnbox">
    {% for letter, list in titles %}
        <div class="columnheading" id="{{ letter }}">{{ letter }}</div>
        {% for paper in list %}
            {{ newspaperLine(paper) }}
        {% endfor %}
    {% endfor %}
</div>
